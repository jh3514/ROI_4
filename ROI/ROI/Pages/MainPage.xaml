<Window x:Class="ROI.Pages.MainPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ROI.Pages"
        mc:Ignorable="d"
        Height="1080" Width="1920">
    <Grid>
        <Label  x:Name="UI_Label_Message" 
                Height="200" Width="1892" Margin="10,10,0,0" 
                HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Image  x:Name="UI_Img_Road"
                Height="813" Width="960" Margin="15,204,0,0"
                VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Image  x:Name="UI_Img_Emergency"
                Height="813" Width="960" Margin="15,204,0,0"
                VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Image  x:Name="UI_Img_Photo"
                Height="851" Width="927" Margin="0,166,10,0"
                VerticalAlignment="Top" HorizontalAlignment="Right"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="856,970,0,0" VerticalAlignment="Top" Width="114" Click="Btn_Clicked" Height="47"/>
    </Grid>
</Window>
